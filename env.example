# NexusCRM Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://54.88.182.69:8000

# Contacts Write Key
# Required for contact write operations (create, update, delete)
# Must match the CONTACTS_WRITE_KEY value configured in the backend
NEXT_PUBLIC_CONTACTS_WRITE_KEY=demo-write-key

# Companies Write Key
# Required for company write operations (create, update, delete)
# Must match the COMPANIES_WRITE_KEY value configured in the backend
NEXT_PUBLIC_COMPANIES_WRITE_KEY=demo-companies-write-key

# Google Gemini API
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key
API_KEY=your_gemini_api_key

